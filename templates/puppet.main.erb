# Manged by puppet - do not modify

[main]
<% if @ca_server and !@ca_server.empty? -%>
  # Use specified CA server
  ca_server = <%= @ca_server %>
<% end -%>
<% if @use_srv_records and @srv_domain -%>
  use_srv_records = true
  srv_domain = <%= @srv_domain %>
<% end -%>
<% if @certname -%>
  certname = <%= @certname %>
<% end -%>
<% if @dns_alt_names && @dns_alt_names.length > 0-%>
  dns_alt_names = <%= @dns_alt_names.join(', ') %>
<% end -%>
<% if @basemodulepath and !@basemodulepath.empty? -%>
  basemodulepath = <%= @basemodulepath %>
<% end -%>
